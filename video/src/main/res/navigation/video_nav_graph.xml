<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/video_nav_graph"
    app:startDestination="@id/playerActivity">

    <activity
        android:id="@+id/playerActivity"
        android:name="com.example.video.PlayerActivity"
        android:label="activity_player"
        tools:layout="@layout/activity_player">
        <deepLink app:uri="fanime://app/video?animeId={animeId}&amp;episodeId={episodeId}" />
        <argument
            android:name="animeId"
            android:defaultValue='""'
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="episodeId"
            android:defaultValue='""'
            app:argType="string"
            app:nullable="true" />
    </activity>
</navigation>